{
  "manifest_version": 3,
  "name": "Thumbnails Blocked",
  "description": "Inject a script to block youtube.com thumbnails to avoid distraction.",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "icons" {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}